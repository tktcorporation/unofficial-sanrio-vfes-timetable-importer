{
	"name": "example-cloudflare-workers",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"deploy": "nr build && wrangler deploy",
		"dev": "react-router dev",
		"start": "wrangler dev --ip=0.0.0.0",
		"typecheck": "react-router typegen && tsc",
		"lint": "biome check .",
		"lint:type-check": "tsc --noEmit",
		"lint:fix": "biome check --write --unsafe . && nr lint:type-check",
		"preview": "nr build && wrangler dev --ip=0.0.0.0",
		"test": "nr test:vitest && nr test:e2e",
		"test:vitest": "vitest run --exclude \"*e2e*\"",
		"test:e2e": "nr test:e2e:vite && nr test:e2e:workers",
		"test:e2e:vite": "playwright test -c playwright-vite.config.ts e2e.test.ts",
		"test:e2e:workers": "nr build && playwright test -c playwright-workers.config.ts e2e.test.ts",
		"typegen": "react-router typegen",
		"generate:event-ids": "tsx scripts/generate-event-ids.ts"
	},
	"dependencies": {
		"@fullcalendar/core": "^6.1.20",
		"@fullcalendar/daygrid": "^6.1.20",
		"@fullcalendar/react": "^6.1.20",
		"@fullcalendar/timegrid": "^6.1.20",
		"@hono/zod-validator": "^0.7.6",
		"@radix-ui/react-accordion": "^1.2.12",
		"@radix-ui/react-slot": "^1.2.4",
		"@radix-ui/react-toggle": "^1.1.10",
		"@react-router/cloudflare": "^7.12.0",
		"@react-router/fs-routes": "^7.12.0",
		"@tailwindcss/vite": "^4.1.18",
		"date-fns": "^4.1.0",
		"hono": "^4.11.7",
		"hono-react-router-adapter": "^0.6.5",
		"isbot": "^5.1.32",
		"lucide-react": "^0.562.0",
		"lz-string": "^1.5.0",
		"playwright": "^1.57.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-router": "^7.12.0",
		"tailwindcss": "^4.1.18",
		"tailwindcss-animate": "^1.0.7",
		"ts-pattern": "^5.9.0",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@hono/vite-dev-server": "^0.24.1",
		"@playwright/test": "^1.57.0",
		"@react-router/dev": "^7.12.0",
		"@shadcn/ui": "^0.0.4",
		"@types/node": "^25.0.9",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"tailwind-merge": "^3.4.0",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1",
		"vite-tsconfig-paths": "^6.0.4",
		"vitest": "^4.0.17",
		"wrangler": "^4.59.2"
	},
	"engines": {
		"node": ">=22.0.0"
	}
}
