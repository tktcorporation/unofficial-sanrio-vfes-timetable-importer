{
	"name": "example-cloudflare-workers",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"deploy": "npm run build && wrangler deploy",
		"dev": "react-router dev",
		"start": "wrangler dev",
		"typecheck": "react-router typegen && tsc",
		"lint": "biome check .",
		"lint:type-check": "tsc --noEmit",
		"lint:fix": "biome check --write --unsafe . && nr lint:type-check",
		"preview": "npm run build && wrangler dev --ip=0.0.0.0",
		"test": "nr test:e2e:vite && nr test:e2e:workers",
		"test:e2e:vite": "playwright test -c playwright-vite.config.ts e2e.test.ts",
		"test:e2e:workers": "npm run build && playwright test -c playwright-workers.config.ts e2e.test.ts",
		"typegen": "react-router typegen",
		"generate:event-ids": "tsx scripts/generate-event-ids.ts"
	},
	"dependencies": {
		"@hono/zod-validator": "^0.4.2",
		"@react-router/cloudflare": "^7.0.1",
		"@react-router/fs-routes": "^7.0.1",
		"@tailwindcss/vite": "^4.0.3",
		"date-fns": "^4.1.0",
		"hono": "^4.6.20",
		"hono-react-router-adapter": "^0.6.2",
		"isbot": "^4.1.0",
		"lucide-react": "^0.474.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router": "^7.0.1",
		"tailwindcss": "^4.0.3",
		"uuid": "^11.0.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@hono/vite-dev-server": "^0.18.1",
		"@playwright/test": "^1.48.2",
		"@react-router/dev": "^7.0.1",
		"@types/node": "^22.13.1",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"@types/uuid": "^10.0.0",
		"playwright": "^1.47.0",
		"typescript": "^5.1.6",
		"vite": "^6.0.7",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^3.86.0"
	},
	"engines": {
		"node": ">=22.0.0"
	}
}
